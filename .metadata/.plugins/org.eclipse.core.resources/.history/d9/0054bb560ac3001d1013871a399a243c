#ifndef DS3231_H_
#define DS3231_H_

#include "main.h"

#include "stdio.h"
#include "string.h"

// address
#define DS3231_ADDR 0x68<<1

//struct
typedef struct
{
	uint8_t sec;
	uint8_t min;
	uint8_t hour;
	uint8_t date;
	uint8_t day;
	uint8_t month;
	uint8_t year;
}DS3231_typedef;




#endif /* DS3231_H_ */
